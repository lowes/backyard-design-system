.c0 input:checked:disabled + label,
.c0 input:checked.disabled + label {
  cursor: not-allowed;
}

.c0 input:checked:disabled + label::before,
.c0 input:checked.disabled + label::before {
  background-color: var(--bds-color-text-disabled);
}

.c0 input:checked:disabled + label::after,
.c0 input:checked.disabled + label::after {
  border-color: var(--bds-color-text-disabled);
}

.c0 input:checked,
.c0 input:checked + label {
  cursor: pointer;
}

.c0 input:checked.size--large + label::after {
  margin-left: var(--bds-sizes-size-24);
}

.c0 input:checked.size--small + label::after {
  margin-left: var(--bds-sizes-size-16);
}

<span
    class="c0 switch--wrapper size--large"
  >
    <input
      class="backyard switch size--large"
      type="checkbox"
      value=""
    />
    <label />
  </span>
</DocumentFragment>
