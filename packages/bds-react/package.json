{
    "name": "@lowes-tech/bds-react",
    "version": "0.0.0",
    "description": "React component for backyard design system",
    "author": "Backyard Design System",
    "main": "./src/",
    "sideEffects": false,
    "keywords": [
        "backyard",
        "react",
        "backyard-react",
        "bds-react",
        "components"
    ],
    "contributors": [
        "Bryant Derosier <bryant.derosier@lowes.com>",
        "William Woodard <william.woodard@lowes.com>"
    ],
    "scripts": {
        "start": "echo \"@lowes-tech/bds-react: Starting...\" && npm-run-all start:server",
        "start:server": "nodemon src/server/index.js",
        "build": "npm-run-all build:init build:typescript build:cjs build:esm build:es build:finalize",
        "build:init": "babel-node scripts/build-init.js",
        "build:typescript": "tsc --project tsconfig.json",
        "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --extensions \".js,.jsx,.ts,.tsx\" ./src --out-dir ./dist/dist --ignore \"**/*.stories.js,**/*.test.js,**/*.spec.js,**/node_modules\"",
        "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --extensions \".js,.jsx,.ts,.tsx\" ./src --out-dir ./dist/esm --ignore \"**/*.stories.js,**/*.test.js,**/*.spec.js,**/node_modules\"",
        "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --extensions \".js,.jsx,.ts,.tsx\" ./src --out-dir ./dist/es --ignore \"**/*.stories.js,**/*.test.js,**/*.spec.js,**/node_modules\"",
        "build:finalize": "babel-node scripts/build-finalize.js",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
        "size:snapshot": "babel-node scripts/sizeSnapshot/create.js",
        "size:stats": "webpack --config scripts/sizeSnapshot/webpack.config.js --json > stats.json",
        "shrinkwrap": "npm shrinkwrap",
        "test": "jest",
        "test:changed": "--changedSince main",
        "test:all": "jest",
        "test:all:update": "SNAPSHOT_UPDATE=true pnpm test:all -- -u",
        "test:update": "SNAPSHOT_UPDATE=true pnpm test -- -u",
        "test:ci": "pnpm test -- --runInBand",
        "test:sizes": "pnpm test:update src/sizes",
        "typedoc": "typedoc",
        "typedoc:create": "babel-node scripts/create-typedocs --",
        "typedoc:create:all": "babel-node scripts/create-typedocs"
    },
    "peerDependencies": {
        "@lowes-tech/bds-tokens": ">=2.9.0 || >=2.14.0-alpha",
        "@lowes-tech/bds-icons": ">=2.9.0 || >=2.14.0-alpha",
        "react": ">= 16.9",
        "react-dom": ">=16.9",
        "styled-components": ">= 4.0.0"
    },
    "dependencies": {
        "@juggle/resize-observer": "^3.3.1",
        "@popperjs/core": "^2.9.2",
        "@types/react-input-mask": "^2.0.5",
        "@types/react-table": "~7.7.2",
        "classnames": "^2.3.1",
        "core-js": "^3.6.5",
        "core-js-pure": "^3.6.5",
        "date-fns": "^2.14.0",
        "prop-types": "^15.7.2",
        "react-device-detect": "^1.13.1",
        "react-dropzone": "^11.2.1",
        "react-input-mask": "^3.0.0-alpha.2",
        "react-is": "^16.13.1",
        "react-popper": "^2.2.3",
        "react-table": "~7.7.0",
        "styled-breakpoints": "^10.0.1",
        "validator": "^13.5.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.0",
        "@babel/core": "^7.13.1",
        "@babel/node": "^7.13.0",
        "@babel/plugin-external-helpers": "^7.12.13",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.13.0",
        "@babel/plugin-proposal-private-methods": "^7.13.0",
        "@babel/plugin-proposal-private-property-in-object": "^7.14.0",
        "@babel/plugin-transform-modules-commonjs": "^7.14.0",
        "@babel/plugin-transform-object-assign": "^7.12.13",
        "@babel/plugin-transform-react-constant-elements": "^7.12.13",
        "@babel/plugin-transform-runtime": "^7.13.7",
        "@babel/preset-env": "^7.12.17",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.13.0",
        "@babel/register": "^7.13.0",
        "@babel/runtime": "^7.13.0",
        "@lowes-tech/bds-tokens": "file:../bds-tokens/src",
        "@lowes-tech/bds-icons": "file:../bds-icons/src",
        "@octokit/core": "^3.2.5",
        "@testing-library/dom": "^7.31.2",
        "@testing-library/jest-dom": "^5.13.0",
        "@testing-library/react": "^11.2.7",
        "@testing-library/react-hooks": "^7.0.0",
        "@types/express": "^4.17.11",
        "@types/fs-extra": "^9.0.7",
        "@types/glob": "^7.1.3",
        "@types/jest": "^26.0.20",
